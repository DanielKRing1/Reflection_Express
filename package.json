{
    "name": "reflection-api",
    "version": "1.6.0",
    "description": "",
    "main": "src/index.ts",
    "scripts": {
        "----VERSIONING": "...",
        "version": "echo %npm_package_version%",
        "commit": "git add . && git commit -m \"New build -> %npm_package_version%\"",
        "bump-patch": "npm run commit && npm version patch",
        "bump-minor": "npm run commit && npm version minor",
        "bump-major": "npm run commit && npm version major",
        "----DEPLOYING": "",
        "t": ".\\start.sh",
        "build": "tsc -p .",
        "build_docker": ".\\build_docker.sh",
        "deploy_fly": ".\\deploy_fly.sh",
        "restart_fly": "fly apps restart reflection-api",
        "deploy": "deprecated npm run build & fly deploy",
        "----STARTING JS": "...",
        "start_dev": "npm run build & node ./dist/src/index.js",
        "start_fly": "node ./dist/src/index.js",
        "----STARTING TS": "...",
        "local_dev": "set NODE_ENV=dev&&nodemon src/index.ts",
        "----TESTING": "...",
        "test": "npx jest --setupFiles ./jest/setup.ts --runInBand",
        "migrate": "npx prisma migrate dev",
        "migrate:dev": "dotenv -e env/.env.dev npx prisma migrate dev",
        "migrate:test": "dotenv -e env/.env.test npx prisma migrate dev",
        "----HELPING": "...",
        "copy-prisma": "xcopy /E /I .\\prisma\\schema.prisma .\\dist\\prisma"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@apollo/server": "^4.5.0",
        "@prisma/client": "4.12",
        "axios": "^1.3.4",
        "bcrypt": "^5.1.0",
        "connect-redis": "^7.0.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "graphql": "^16.6.0",
        "helmet": "^6.0.1",
        "jsonwebtoken": "^9.0.0",
        "morgan": "^1.10.0",
        "redis": "^4.6.5",
        "set-cookie-parser": "^2.6.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^29.5.0",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/morgan": "^1.9.4",
        "@types/set-cookie-parser": "^2.4.2",
        "@types/supertest": "^2.0.12",
        "jest": "^29.5.0",
        "nodemon": "^2.0.21",
        "prisma": "^4.15.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4"
    }
}
